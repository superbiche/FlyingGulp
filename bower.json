{
  "name": "bla-gulp-workflow",
  "private": true,
  "dependencies": {
    "modernizr": "latest",
    "jquery": "1.11.1",
    "colorbox": "latest",
    "jquery-cycle2": "latest",
    "URIjs": "latest",
    "jquery-ui-selectmenu": "https://github.com/fnagel/jquery-ui.git#selectmenu"
  },
  "exportsOverride": {
    "modernizr": {
      "js": "**/*.js"
    },
    "jquery": {
      "minjs": "dist/*.{min.js,map}"
    },
    "*": {
      "minjs": "**/*.{min.js,map}",
      "js": "**/*.js",
      "css": "**/*.css",
      "scss": "**/*.scss"
    }
  }
}