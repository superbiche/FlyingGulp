{
  "name": "komilfo_theme",
  "private": true,
  "dependencies": {
    "modernizr": "latest",
    "jquery": "1.11.1",
    "colorbox": "latest",
    "lightgallery": "latest",
    "jquery-cycle2": "latest",
    "mustache.js": "latest"
  },
  "exportsOverride": {
    "modernizr": {
      "js": "**/*.js"
    },
    "jquery": {
      "minjs": "dist/*.{min.js,map}"
    },
    "*": {
      "minjs": "**/*.{min.js,map}",
      "js": "**/*.js",
      "css": "**/*.css",
      "scss": "**/*.scss"
    }
  }
}